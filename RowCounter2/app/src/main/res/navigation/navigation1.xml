<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/navigation1"
    app:startDestination="@id/titleFragment"
    tools:ignore="UnusedNavigation">

    <fragment
        android:id="@+id/titleFragment"
        android:name="android.example.com.rowcounter2.TitleFragment"
        android:label="fragment_title"
        tools:layout="@layout/fragment_title" >
        <action
            android:id="@+id/action_titleFragment_to_transformtest"
            app:destination="@id/transformtest"
            app:popUpToInclusive="true" />
    </fragment>
    <fragment
        android:id="@+id/transformtest"
        android:name="android.example.com.rowcounter2.transform1.Transformtest"
        android:label="Transformtest"
        tools:layout="@layout/fragment_transformtest">
        <argument
            android:name="countTransfer"
            app:argType="integer" />
        <action
            android:id="@+id/action_transformtest_to_titleFragment"
            app:destination="@id/titleFragment"
            app:popUpToInclusive="true" />
        <argument
            android:name="test"
            app:argType="integer" />
    </fragment>
</navigation>